#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint@7.24.0/node_modules/eslint/bin/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint@7.24.0/node_modules/eslint/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint@7.24.0/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules:/home/runner/work/blog/blog/src/node_modules:/home/runner/work/blog/blog/node_modules:/home/runner/work/blog/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_dc9d9d711f9715ea38ced5881f20889e/node_modules/eslint-config-airbnb-base/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_dc9d9d711f9715ea38ced5881f20889e/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint@7.24.0/node_modules/eslint/bin/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint@7.24.0/node_modules/eslint/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint@7.24.0/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules:/home/runner/work/blog/blog/src/node_modules:/home/runner/work/blog/blog/node_modules:/home/runner/work/blog/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_dc9d9d711f9715ea38ced5881f20889e/node_modules/eslint-config-airbnb-base/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_dc9d9d711f9715ea38ced5881f20889e/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.24.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@7.24.0/node_modules/eslint/bin/eslint.js" "$@"
fi
