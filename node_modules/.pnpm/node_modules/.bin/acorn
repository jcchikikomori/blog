#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/blog/blog/src/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules:/home/runner/work/blog/blog/src/node_modules:/home/runner/work/blog/blog/node_modules:/home/runner/work/blog/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules/acorn/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/work/blog/blog/src/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/bin/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/acorn@7.4.1/node_modules/acorn/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/acorn@7.4.1/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules:/home/runner/work/blog/blog/src/node_modules:/home/runner/work/blog/blog/node_modules:/home/runner/work/blog/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/home/runner/work/blog/blog/src/node_modules/.pnpm/node_modules/acorn/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
